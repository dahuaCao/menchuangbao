<view class='weui-cell-third' style='max-height:{{maxHeight}};'>
  <block wx:for="{{lists}}" wx:key="*this">
    <view class='weui-cell-list'>
      <view class='weui-cell-circle'></view>
      <view class='weui-cell-line'>
        <view class='item'>{{item.schedule}}</view>
        <view class='item'>{{item.processTime}}</view>
      </view>
    </view>
  </block>
  <view class='btn' catchtap='change'>
    订单轨迹
    <text wx:if='{{!toggle}}' class="iconfont iconbelow-s"></text>
    <text wx:else class="iconfont icontop-s"></text>
  </view>
</view>