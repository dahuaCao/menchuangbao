/* pages/dingdan/dingdan.wxss */
.container{
    width:100%;
    /* min-height:395rpx; */
    height:auto;
    background:rgba(255,255,255,1);
    box-shadow:0px 0px 20rpx 0px rgba(0, 0, 0, 0.35);
    
    padding:0 28rpx 32rpx 28rpx;
    margin-bottom: 20rpx;
}
.container .title{
    display:flex;
    height:75rpx;
    border-bottom:1px solid rgba(133,142,162,1);
    justify-content:space-between;
    align-items:center;
}
.container .title .status{
    color:#FE0707;
}
.container .content{
    padding-top:27rpx;
    display:flex;
}
.container .content .left{
    width:190rpx;
    height:190rpx;
    margin-right:24rpx;
    background:#ccc;
}
.container .content .right{
    flex:1;
    height:190rpx;
}
.container .content .right .item{
    margin-top:12rpx;
}
.container .content .right .judge{
    text-align:right;
}
.container .content .right .judge .des{
    color:#FF0000;
    padding:8rpx 28rpx;
    border:1px solid rgba(254,7,7,1);
    border-radius:7px;
}
.container .bottom{
    margin: 10rpx;
}
